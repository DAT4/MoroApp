<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MinProfilKalenderFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#FAB96B"
        android:gravity="center">

        <CalendarView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/profileCalender"
            android:theme="@style/Theme.Custom"
            android:dateTextAppearance="@style/calender_theme_date"
            android:weekDayTextAppearance="@style/calender_theme_week"
            />

    </LinearLayout>
</FrameLayout>